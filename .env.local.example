# SniperIQ Landing Page Environment Variables
# Copy this file to .env.local and fill in your actual values

# API Configuration
NEXT_PUBLIC_API_URL=https://api.sniperiq.ai
NEXT_PUBLIC_API_KEY=your_api_key_here

# Application URLs
NEXT_PUBLIC_URL=https://sniperiq.ai
NEXT_PUBLIC_APP_URL=https://app.sniperiq.ai

# Analytics & Tracking
NEXT_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Error Tracking
NEXT_PUBLIC_ERROR_WEBHOOK_URL=your_error_webhook_url

# Support & Chat
NEXT_PUBLIC_CRISP_WEBSITE_ID=your_crisp_website_id

# Optional: Feature Flags
# NEXT_PUBLIC_ENABLE_BETA=false
